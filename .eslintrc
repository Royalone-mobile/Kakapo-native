{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "react/prop-types": 0,
    "react/jsx-uses-react": 1,
    "react/jsx-no-bind": 0,
    "array-bracket-spacing": [2, "always"],
    "semi": [2, "always"],
    "comma-dangle": [2, "never"],
    "no-shadow": 0,
    "no-param-reassign": 0,
    "newline-per-chained-call": 0,
    "no-confusing-arrow": 0
  },
  "plugins": [
    "react"
  ]
}
